{
  "version": 2,
  "builds": [
    { "src": "api/**/*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/data/(.*)", "dest": "api/index.js" },
    { "src": "/api/(.*)", "dest": "api/index.js" }
  ],
  "functions": {
    "api/**/*.js": {
      "memory": 512,
      "maxDuration": 10
    }
  }
}
